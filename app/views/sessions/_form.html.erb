<%= form_for(:session, url: sessions_path) do |f|%>
  <div class="field">
    <%= f.label :username, "Email" %><br>
    <%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Sign in", class: "btn btn-primary" %>
  </div>
<% end %>