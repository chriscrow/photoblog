<li>
    <p><%= article.content %></p>
    <p class="timestamp">Posted at <%= time_ago_in_words article.created_at %> ago.</p>
    <% if current_user?(article.user) %>
        <%= link_to "delete", article, method: :delete, data: { confirm: "Are you true" }, title: article.content %>
    <% end %>
</li>